<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ryocomp.makecombination">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_make_draw"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_make_draw"
        android:supportsRtl="true"
        android:theme="@style/Theme.MakeCombination">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SettingActivity"></activity>
        <activity android:name=".MakeCombActivity"></activity>
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3891518799622736~1939715472"/>
    </application>

</manifest>